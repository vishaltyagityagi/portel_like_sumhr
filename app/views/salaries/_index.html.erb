<div id= "container">
<h1>Listing Details</h1>
<p id="notice"><%= notice %></p>


<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Monthly</th>
      <th>CTC</th>
      <th>PF</th>
      <th>TDS</th>
      <th>Ip address</th>
      <th>Action</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
      <tr>
        <td><%=salary.created_at.strftime('%d-%m-%y')%></td>
        <td><%=salary.per_month%></td>
        <td><%=salary.ctc%></td>
        <td><%=salary.pf%></td>
        <td><%=salary.tds%></td>
        <td><%=current_user.current_sign_in_ip%></td>
        <td><%=link_to "Back", :back%></td>
       
      </tr>
  </tbody>
</table>

<br>
</div>


